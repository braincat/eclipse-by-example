  </xsd:simpleType>
 
  <xsd:complexType name="exposureProgramType">
    <xsd:simpleContent>
      <xsd:extension base="exposureProgram_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
