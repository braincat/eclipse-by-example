  <xsd:simpleType name="gpsLongitude_t">
    <xsd:restriction base="xsd:float">
      <xsd:minInclusive value="0.0"/>
      <xsd:maxInclusive value="180.0"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsBearingType">
    <xsd:simpleContent>
      <xsd:extension base="gpsBearing_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsBearing_t">
    <xsd:restriction base="xsd:float">
      <xsd:minInclusive value="0.0"/>
      <xsd:maxExclusive value="360.0"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsStatusType">
    <xsd:simpleContent>
      <xsd:extension base="gpsStatus_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsStatus_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Measurement in progress"/>
      <xsd:enumeration value="Measurement interoperability"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsMeasureModeType">
    <xsd:simpleContent>
      <xsd:extension base="gpsMeasureMode_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsMeasureMode_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="2-dimensional measurement"/>
      <xsd:enumeration value="3-dimensional measurement"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsSpeedRefType">
    <xsd:simpleContent>
      <xsd:extension base="gpsSpeedRef_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsSpeedRef_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Kilometers per hour"/>
      <xsd:enumeration value="Miles per hour"/>
      <xsd:enumeration value="Knots"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsDirectionType">
    <xsd:simpleContent>
      <xsd:extension base="gpsDirection_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsDirection_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="True direction"/>
      <xsd:enumeration value="Magnetic direction"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsDistanceRefType">
    <xsd:simpleContent>
      <xsd:extension base="gpsDistanceRef_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsDistanceRef_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Kilometers"/>
      <xsd:enumeration value="Miles"/>
      <xsd:enumeration value="Knots"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <xsd:complexType name="gpsDifferentialType">
    <xsd:simpleContent>
      <xsd:extension base="gpsDifferential_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="gpsDifferential_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Measurement without differential correction"/>
      <xsd:enumeration value="Differential correction applied"/>
    </xsd:restriction>
  </xsd:simpleType>
 
  <!-- 
       TYPES FOR THE INTEROPERABILTY IFD
     -->
  <xsd:complexType name="interoperabilityType">
    <xsd:simpleContent>
      <xsd:extension base="interoperability_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="interoperability_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="R98"/>
      <xsd:enumeration value="THM"/>
      <xsd:enumeration value="R03"/>
    </xsd:restriction>
  </xsd:simpleType>
 
 
  <!-- GENERIC TYPE
 
        A generic type to hold any type of tag data. 
        Defines a name, value, datatype triplet 
        Datatype values refer to types as defined by XML Schema.
 
        singleFieldType is for EXIF tag that define a single 
        value, the common case.
 
        repeatedFieldTyp is for the uncommon case where many
        data items are defined in an EXIF tag.
 
    -->
  <xsd:complexType name="singleFieldType">
    <xsd:complexContent>
      <xsd:extension base="singleField_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
    
  <xsd:complexType name="repeatedFieldType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="1" name="Field" type="singleField_t"/>
    </xsd:sequence>
    <xsd:attributeGroup ref="exifAttrs"/>
  </xsd:complexType>
 
  <xsd:complexType name="singleField_t">
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string"/>
      <xsd:element name="Value" type="xsd:string"/>
      <xsd:element name="Datatype">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>The enumerated datatype values refer to types 
                defined by XML Schema
            </xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="string"/>
            <xsd:enumeration value="integer"/>
            <xsd:enumeration value="float"/>
            <xsd:enumeration value="date"/>
            <xsd:enumeration value="time"/>
            <xsd:enumeration value="dateTime"/>
            <xsd:enumeration value="hexBinary"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
 
  <!-- END TYPE DEFINITIONS -->
 
  <!-- 
       THE GLOBAL ELEMENT
    -->
  <xsd:element name="exifMetadata">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="TiffIfd">
          <xsd:annotation>
            <xsd:documentation>Tags from the TIFF IFD</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:all>
 
              <!-- Tags relating to image data structure -->
              <xsd:element minOccurs="0" name="ImageWidth" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="ImageLength" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="BitsPerSample" type="bitsPerSampleType"/>
              <xsd:element minOccurs="0" name="Compression" type="compressionType"/>
              <xsd:element minOccurs="0" name="PhotometricInterpretation" type="photometricInterpretationType"/>
              <xsd:element minOccurs="0" name="Orientation" type="orientationType"/>
              <xsd:element minOccurs="0" name="SamplesPerPixel" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="PlanarConfiguration" type="planarConfigurationType"/>
              <xsd:element minOccurs="0" name="YCbCrSubSampling" type="yCbCrSubSamplingType"/>
              <xsd:element minOccurs="0" name="YCbCrPositioning" type="yCbCrPositioningType"/>
              <xsd:element minOccurs="0" name="XResolution" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>Unit is pixels per ResolutionUnit</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="YResolution" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>Unit is pixels per Resolution Unit</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="ResolutionUnit" type="resolutionType"/>
 
              <!--Tags relating to recording offset -->
              <xsd:element minOccurs="0" name="StripOffsets" type="stripOffsetsType"/>
              <xsd:element minOccurs="0" name="RowsPerStrip" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="StripByteCounts" type="stripByteCountsType"/>
              <xsd:element minOccurs="0" name="JPEGInterChangeFormat" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="JPEGInterChangeFormatLength" type="positiveIntegerType"/>
 
              <!-- Tags relating to image data characteristics -->
              <xsd:element minOccurs="0" name="TransferFunction" type="xsd:anyType"/>
              <xsd:element minOccurs="0" name="WhitePoint" type="whitePointType"/>
              <xsd:element minOccurs="0" name="PrimaryChromaticities" type="primaryChromaticitiesType"/>
              <xsd:element minOccurs="0" name="YCbCrCoefficients" type="yCbCrCoefficientsType"/>
              <xsd:element minOccurs="0" name="ReferenceBlackWhite" type="primaryChromaticitiesType"/>
 
              <!--Other tags -->
              <xsd:element minOccurs="0" name="DateTime" type="dateTimeType"/>
              <xsd:element minOccurs="0" name="ImageDescription" type="stringType"/>
              <xsd:element minOccurs="0" name="Make" type="stringType"/>
              <xsd:element minOccurs="0" name="Model" type="stringType"/>
              <xsd:element minOccurs="0" name="Software" type="stringType"/>
              <xsd:element minOccurs="0" name="Artist" type="stringType"/>
              <xsd:element minOccurs="0" name="Copyright" type="stringType"/>
 
              <!-- Placeholder tags for future tags that may be defined -->
              <xsd:element minOccurs="0" name="TiffField1" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="TiffField2" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="TiffField3" type="repeatedFieldType"/>
 
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
 
        <xsd:element name="ExifIfd">
          <xsd:annotation>
            <xsd:documentation>Tags from the EXIF IFD</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:all>
 
              <!-- Tags releating to version -->
              <xsd:element minOccurs="0" name="ExifVersion" type="stringType"/>
              <xsd:element minOccurs="0" name="FlashpixVersion" type="stringType"/>
 
              <!-- Tags relating to image data characteristics -->
              <xsd:element minOccurs="0" name="ColorSpace" type="colorSpaceType"/>
 
              <!-- Tags relating to image configuration -->
              <xsd:element minOccurs="0" name="ComponentsConfiguration" type="stringType"/>
              <xsd:element minOccurs="0" name="CompressedBitsPerPixel" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="PixelXDimension" type="nonNegativeIntegerType"/>
              <xsd:element minOccurs="0" name="PixelYDimension" type="nonNegativeIntegerType"/>
 
              <!-- Tags relating to user information -->
              <xsd:element minOccurs="0" name="MakerNote" type="repeatedFieldType"/>
              <xsd:element minOccurs="0" name="UserComment" type="stringType"/>
 
              <!-- Tag relating to related file information -->
              <xsd:element minOccurs="0" name="RelatedSoundFile" type="stringType"/>
 
              <!-- Tags relating to date and time -->
              <xsd:element minOccurs="0" name="DateTimeOriginal" type="dateTimeType"/>
              <xsd:element minOccurs="0" name="DateTimeDigitized" type="dateTimeType"/>
              <xsd:element minOccurs="0" name="SubSecTime" type="nonNegativeIntegerType"/>
              <xsd:element minOccurs="0" name="SubSecTimeOriginal" type="nonNegativeIntegerType"/>
              <xsd:element minOccurs="0" name="SubSecTimeDigitized" type="nonNegativeIntegerType"/>
 
              <!-- Tags relating to picture taking conditions -->
              <xsd:element minOccurs="0" name="ExposureTime" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>Units is seconds</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="FNumber" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="ExposureProgram" type="exposureProgramType"/>
              <xsd:element minOccurs="0" name="SpectralSensitivity" type="stringType"/>
              <xsd:element minOccurs="0" name="ISOSpeedRatings" type="nonNegativeIntegerType"/>
              <xsd:element minOccurs="0" name="OECF" type="repeatedFieldType"/>
              <xsd:element minOccurs="0" name="ShutterSpeedValue" type="realType">
                <xsd:annotation>
                  <xsd:documentation>The unit is the APEX Value</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="ApertureValue" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The  unit is the APEX value</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="BrightnessValue" type="realType">
                <xsd:annotation>
                  <xsd:documentation>The unit is the APEX value</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="ExposureBiasValue" type="realType">
                <xsd:annotation>
                  <xsd:documentation>The unit is the APEX value</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="MaxApertureValue" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The unit is the APEX value</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="SubjectDistance" type="stringType">
                <xsd:annotation>
                  <xsd:documentation>The unit is meters</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="MeteringMode" type="meteringModeType"/>
              <xsd:element minOccurs="0" name="LightSource" type="lightSourceType"/>
              <xsd:element minOccurs="0" name="Flash" type="flashType"/>
              <xsd:element minOccurs="0" name="FocalLength" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The unit is millimeters.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="SubjectArea" type="subjectAreaType"/>
              <xsd:element minOccurs="0" name="FlashEnergy" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The unit is Beam Candle Power Seconds</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="SpatialFrequencyResponse" type="repeatedFieldType">
                <xsd:annotation>
                  <xsd:documentation>Not implemented</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="FocalPlaneXResolution" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The unit is pixels</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="FocalPlaneYResolution" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>The unit is pixels</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="FocalPlaneResolutionUnit" type="resolutionType"/>
              <xsd:element minOccurs="0" name="SubjectLocation" type="subjectLocationType"/>
              <xsd:element minOccurs="0" name="ExposureIndex" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="SensingMethod" type="sensingMethodType"/>
              <xsd:element minOccurs="0" name="FileSource" type="fileSourceType"/>
              <xsd:element minOccurs="0" name="SceneType" type="stringType"/>
              <xsd:element minOccurs="0" name="CFAPattern" type="repeatedFieldType"/>
              <xsd:element minOccurs="0" name="CustomRendered" type="customRenderedType"/>
              <xsd:element minOccurs="0" name="ExposureMode" type="exposureModeType"/>
              <xsd:element minOccurs="0" name="WhiteBalance" type="whiteBalanceType"/>
              <xsd:element minOccurs="0" name="DigitalZoomRatio" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="FocalLengthIn35mmFilm" type="positiveIntegerType"/>
              <xsd:element minOccurs="0" name="SceneCaptureType" type="sceneCaptureType"/>
              <xsd:element minOccurs="0" name="GainControl" type="gainControlType"/>
              <xsd:element minOccurs="0" name="Contrast" type="contrastType"/>
              <xsd:element minOccurs="0" name="Saturation" type="saturationType"/>
              <xsd:element minOccurs="0" name="Sharpness" type="sharpnessType"/>
              <xsd:element minOccurs="0" name="DeviceSettingDescription" type="repeatedFieldType"/>
              <xsd:element minOccurs="0" name="SubjectDistanceRange" type="subjectDistanceRangeType"/>
              <xsd:element minOccurs="0" name="ImageUniqueID" type="uuidType"/>
              <xsd:element minOccurs="0" name="Gamma" type="nonNegativeRealType"/>
 
              <!-- Placeholder tags for future tags that may be defined -->
              <xsd:element minOccurs="0" name="ExifField1" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="ExifField2" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="ExifField3" type="repeatedFieldType"/>
 
            </xsd:all>
            <xsd:attributeGroup ref="exifAttrs"/>
          </xsd:complexType>
        </xsd:element>
 
        <xsd:element minOccurs="0" name="GpsIfd">
          <xsd:annotation>
            <xsd:documentation>Tags from the GPS IFD</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="GPSVersionID" type="stringType"/>
              <xsd:element minOccurs="0" name="GPSLatitudeRef" type="gpsLatitudeRefType"/>
              <xsd:element minOccurs="0" name="GPSLatitude" type="gpsLatitudeType">
                <xsd:annotation>
                  <xsd:documentation>Unit is decimal degrees</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="GPSLongitudeRef" type="gpsLongitudeRefType"/>
              <xsd:element minOccurs="0" name="GPSLongitude" type="gpsLongitudeType">
                <xsd:annotation>
                  <xsd:documentation>Unit is decimal degrees</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="GPSAltitudeRef" type="gpsAltitudeRefType"/>
              <xsd:element minOccurs="0" name="GPSAltitude" type="nonNegativeRealType">
                <xsd:annotation>
                  <xsd:documentation>Unit is meters</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="GPSTimeStamp" type="timeType"/>
              <xsd:element minOccurs="0" name="GPSSatellites" type="stringType"/>
              <xsd:element minOccurs="0" name="GPSStatus" type="gpsStatusType"/>
              <xsd:element minOccurs="0" name="GPSMeasureMode" type="gpsMeasureModeType"/>
              <xsd:element minOccurs="0" name="GPSDOP" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="GPSSpeedRef" type="gpsSpeedRefType"/>
              <xsd:element minOccurs="0" name="GPSSpeed" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="GPSTrackRef" type="gpsDirectionType"/>
              <xsd:element minOccurs="0" name="GPSTrack" type="gpsBearingType"/>
              <xsd:element minOccurs="0" name="GPSImgDirectionRef" type="gpsDirectionType"/>
              <xsd:element minOccurs="0" name="GPSImgDirection" type="gpsBearingType"/>
              <xsd:element minOccurs="0" name="GPSMapDatum" type="stringType"/>
              <xsd:element minOccurs="0" name="GPSDestLatitudeRef" type="gpsLatitudeRefType"/>
              <xsd:element minOccurs="0" name="GPSDestLatitude" type="gpsLatitudeType"/>
              <xsd:element minOccurs="0" name="GPSDestLongitudeRef" type="gpsLongitudeRefType"/>
              <xsd:element minOccurs="0" name="GPSDestLongitude" type="gpsLongitudeType"/>
              <xsd:element minOccurs="0" name="GPSDestBearingRef" type="gpsDirectionType"/>
              <xsd:element minOccurs="0" name="GPSDestBearing" type="gpsBearingType"/>
              <xsd:element minOccurs="0" name="GPSDestDistanceRef" type="gpsDistanceRefType"/>
              <xsd:element minOccurs="0" name="GPSDestDistance" type="nonNegativeRealType"/>
              <xsd:element minOccurs="0" name="GPSProcessingMethod" type="stringType"/>
              <xsd:element minOccurs="0" name="GPSAreaInformation" type="stringType"/>
              <xsd:element minOccurs="0" name="GPSDateStamp" type="dateType"/>
              <xsd:element minOccurs="0" name="GPSDifferential" type="gpsDifferentialType"/>
 
              <!-- Placeholder tags for future tags that may be defined -->
              <xsd:element minOccurs="0" name="GPSField1" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="GPSField2" type="singleFieldType"/>
              <xsd:element minOccurs="0" name="GPSField3" type="repeatedFieldType"/>
            </xsd:all>
            <xsd:attributeGroup ref="exifAttrs"/>
          </xsd:complexType>
        </xsd:element>
 
        <xsd:element minOccurs="0" name="InteroperabilityIfd">
          <xsd:annotation>
            <xsd:documentation>Tags from the Interoperability IFD</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:all>
              <xsd:element minOccurs="0" name="InteroperabilityIndex" type="interoperabilityType"/>
            </xsd:all>
            <xsd:attributeGroup ref="exifAttrs"/>
          </xsd:complexType>
        </xsd:element>
 
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
