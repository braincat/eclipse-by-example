      <xsd:extension base="exposureMode_t">
        <xsd:attributeGroup ref="exifAttrs"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="exposureMode_t">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Auto exposure"/>
